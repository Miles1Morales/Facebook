<!DOCTYPE html>
<html>
<head>
  <title>Connexion</title>
</head>
<body>
  <h1>Connexion</h1>
  <%= form_for @user, url: login_path do |f| %>
    <div>
      <%= f.label :email %>
      <%= f.email_field :email %>
    </div>
    <div>
      <%= f.label :password %>
      <%= f.password_field :password %>
    </div>
    <div>
      <%= f.submit "Se connecter" %>
    </div>
  <% end %>
</body>
</html>
